{
  "name": "travel-processor",
  "version": "1.0.0",
  "private": true,
  "main": "webapp/index.html",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "ui5 build preload --clean-dest --include-task=generateCachebusterInfo",
    "start": "ui5 serve",
    "test": "npm run test:node && npm run test:java",
    "test:java": "karma start --server=java --single-run",
    "test:node": "karma start --server=node --single-run",
    "ts-typecheck": "tsc --noEmit",
    "prestart": "npm run ts-typecheck",
    "prebuild": "npm run ts-typecheck",
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf"
  },
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1.18.7",
    "@sapui5/types": "^1.140.0",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "@ui5/cli": "^4.0.26",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-ui5": "^4.1.0",
    "karma-ui5-transpile": "^3.5.14",
    "puppeteer": "^24.21.0",
    "typescript": "^5.9.2",
    "ui5-middleware-simpleproxy": "^3.5.0",
    "ui5-task-zipper": "^3.4.3",
    "ui5-tooling-transpile": "^3.9.1"
  }
}
